!TomorrowNightEighties

#define t_background        #2d2d2d
#define t_current_line      #393939
#define t_selection      	#515151
#define t_foreground      	#cccccc
#define t_comment	     	#999999
#define t_red		     	#f2777a
#define t_orange	     	#f99157
#define t_yellow        	#ffcc66
#define t_green	        	#99cc99
#define t_aqua	        	#66cccc
#define t_blue	        	#6699cc
#define t_purple        	#cc99cc


URxvt*foreground:   t_foreground
URxvt*background:   t_background
URxvt*cursorColor:  #aeafad

! Black / Grey
URxvt*color0:       #000000
URxvt*color8:       #666666

! Red / Bright Red
URxvt*color1:       t_red
URxvt*color9:       #FF3334

! Green + Bright Green
URxvt*color2:       t_green
URxvt*color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
URxvt*color3:       t_orange
URxvt*color11:      t_yellow

! Blue + Bright Blue
URxvt*color4:       t_blue
URxvt*color12:      t_blue

! Magenta (Purple) + Bright Magenta
URxvt*color5:       t_purple
URxvt*color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
URxvt*color6:       t_aqua
URxvt*color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
URxvt*color7:       t_selection
URxvt*color15:      t_current_line